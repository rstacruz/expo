#!/usr/bin/env node

var cli = require('commander');
cli
  .version(require('../package.json').version);

cli
  .option('--skip-sequelize', 'Disable models')
  .option('--skip-assets', 'Disable asset pipeline')
  .option('--skip-sessions', 'Disable sessions')
  .option('--skip-test', 'Disable tests')
  .option('--edge', 'Use edge version of Expo')

cli.parse(process.argv);

cli
  .help();
